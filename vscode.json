{
    "workbench.iconTheme": "vscode-icons",
    "editor.fontFamily": "'MonacoB'",
    "editor.fontSize": 18,
    "editor.wordWrap": "on",
    "window.menuBarVisibility": "default",
    "workbench.statusBar.visible": true,
    "window.zoomLevel": 0,
    "editor.minimap.enabled": false,
    "vsicons.projectDetection.autoReload": true,
    "team.showWelcomeMessage": false,
    "workbench.startupEditor": "newUntitledFile",
    "git.enabled": false,
    "git.path": null,
    "git.autofetch": false,
    "html.format.enable": false,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "php.validate.executablePath": "C:\\xampp\\php\\php.exe",
    "php.executablePath": "C:\\xampp\\php\\php.exe",
    "php.validate.run": "onType",
    "phpformatter.pharPath": "C:\\xampp\\htdocs\\php-cs-fixer-v2.phar",
    "php-cs-fixer.executablePathWindows": "C:\\xampp\\htdocs\\php-cs-fixer-v2.phar",
    "php-cs-fixer.formatHtml": true,
    "php-cs-fixer.onsave": true,
    "vetur.validation.template": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "workbench.colorTheme": "Atom One Dark",
    "java.home": "C:\\Program Files\\Java\\jdk-9.0.1",
    "emmet.includeLanguages": {
        "blade": "html",
        "vue-html": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "blade.format.enable": true,
    "eslint._legacyModuleResolve": true,
    "eslint.alwaysShowStatus": true,
    "eslint.autoFixOnSave": true,
    "vetur.format.defaultFormatter.css": "prettier",
    "vetur.format.defaultFormatter.js": "prettier",
    "eslint.enable": true,
    "eslint.options": {
        "extensions": [
            ".html",
            ".js",
            ".vue",
            ".jsx"
        ]
    },
    "eslint.validate": [{
            "language": "html",
            "autoFix": true
        },
        {
            "language": "vue",
            "autoFix": true
        },
        {
            "language": "javascript",
            "autoFix": true
        },
        {
            "language": "javascriptreact",
            "autoFix": true
        }
    ],
    "eslint.run": "onSave"
}